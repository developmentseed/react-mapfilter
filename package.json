{
  "name": "Monitoring-app",
  "version": "0.2.2",
  "description": "An app for viewing monitoring data",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^5.2.0",
    "brfs": "~1.3.0",
    "browserify": "^9.0.8",
    "budo": "^4.0.0",
    "buildbranch": "0.0.3",
    "copyfiles": "^0.2.1",
    "crx": "~3.0.2",
    "ejsify": "0.1.0",
    "electron-packager": "^5.2.1",
    "garnish": "^2.1.3",
    "hbsfy": "^2.4.1",
    "jstify": "^0.9.0",
    "mkdirp": "^0.5.1",
    "opnr": "^1.0.4",
    "packageify": "^0.2.0",
    "postcss-cli": "^1.3.1",
    "postcss-import": "^5.2.2",
    "postcss-url": "^3.2.0",
    "rimraf": "^2.3.4",
    "standard": "^4.0.1"
  },
  "dependencies": {
    "auth0-lock": "^8.1.2",
    "backbone": "^1.1.2",
    "bootstrap": "^3.3.6",
    "collect-stream": "^1.1.1",
    "crossfilter": "^1.3.11",
    "d3": "3",
    "drag-drop": "^2.12.1",
    "ecstatic": "^1.4.0",
    "electron": "^1.4.6",
    "end-of-stream": "^1.1.0",
    "forkdb": "^9.2.1",
    "hubfs.js": "^1.0.0",
    "hyperlog": "^4.10.5",
    "hyperlog-index": "^5.0.3",
    "jquery": "^2.1.4",
    "json2csv": "^3.0.2",
    "leaflet": "^0.7.3",
    "level": "^1.5.0",
    "lodash": "^3.1.0",
    "mkdirp": "^0.5.1",
    "octokat": "^0.4.10",
    "odk-sync": "^1.0.0",
    "once": "^1.3.3",
    "shp-write": "^0.2.1",
    "xdg-basedir": "^2.0.0",
    "xform-to-json": "^1.2.0",
    "xhr": "^2.2.2"
  },
  "scripts": {
    "package-win": "rimraf \"dist/Monitoring Station-win32-x64\" && electron-packager . \"Monitoring Station\" --out=dist --ignore=\"^/dist\" --platform=win32 --arch=x64 --version=0.36.9 --prune --overwrite --asar",
    "package-mac": "rimraf \"dist/Monitoring Station-darwin-x64\" && electron-packager . \"Monitoring Station\" --out=dist --ignore=\"^/dist\" --platform=darwin --arch=x64 --version=0.36.9 --prune --overwrite",
    "dev": "budo js/index.js:bundle.js --dir dist --live -- -t [ jstify --engine lodash ]",
    "open": "npm run build:dev && npm run dev | opnr | garnish",
    "watch": "watchify js/index.js -o dist/bundle.js -t [ jstify --engine lodash ] -dv",
    "start": "electron app.js --disable-http-cache",
    "build:dev": "mkdirp dist && copyfiles index.html dist && npm run build:css && npm run build:img",
    "build:web": "npm run build:dev && npm run build:js && npm run build:config",
    "prebuild:web": "rimraf dist && mkdirp dist",
    "build:config": "node bin/build_config.js",
    "build:css": "postcss --use postcss-import --use autoprefixer --use postcss-url --postcss-url.url inline css/index.css > dist/bundle.css",
    "build:js": "browserify js/index.js -t [ jstify --engine lodash ] | uglifyjs -mc > dist/bundle.js",
    "build:img": "copyfiles images/* dist",
    "deploy": "buildbranch gh-pages dist",
    "predeploy": "npm run build:web && copyfiles CNAME dist",
    "build:chrome": "copyup chrome/* dist && crx pack dist -o dist/mapfilter.crx",
    "test": "standard",
    "patch": "npm version patch && git push --follow-tags",
    "rebuild-leveldb": "cd node_modules/leveldown && set HOME=~/.electron-gyp && node-gyp rebuild --target=1.4.6 --arch=x64 --dist-url=https://atom.io/download/atom-shell"
  },
  "standard": {
    "ignore": [
      "js/lib/**",
      "dist/**"
    ]
  }
}
